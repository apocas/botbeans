<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Actions">
        <folder name="Window">
            <file name="org-botbeans-control-ServerAction.instance">
                <attr name="component" methodvalue="org.botbeans.control.ServerTopComponent.findInstance"/>
                <attr name="displayName" bundlevalue="org.botbeans.control.Bundle#CTL_ServerAction"/>
                <attr name="iconBase" stringvalue="org/botbeans/control/Bluetooth_32.png"/>
                <attr name="instanceCreate" methodvalue="org.openide.windows.TopComponent.openAction"/>
            </file>
        </folder>
    </folder>
    <folder name="Menu">
        <folder name="Window">
            <file name="ServerAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/org-botbeans-control-ServerAction.instance"/>
            </file>
        </folder>
    </folder>
    <folder name="Windows2">
        <folder name="Components">
            <file name="ServerTopComponent.settings" url="ServerTopComponentSettings.xml"/>
        </folder>
        <folder name="Modes">
            <folder name="explorer">
                <file name="ServerTopComponent.wstcref" url="ServerTopComponentWstcref.xml"/>
            </folder>
        </folder>
    </folder>
</filesystem>
